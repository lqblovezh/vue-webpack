    "precommit": "npm run lint",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js --progress --colors",
    "dev:server": "nodemon server/server.js --progress --colors",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
